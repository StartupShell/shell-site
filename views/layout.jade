doctype html
html
  head
    title Startup Shell
    <link href='http://fonts.googleapis.com/css?family=Titillium+Web:400,700' rel='stylesheet' type='text/css'>
    link(rel='stylesheet',href='/components/bootstrap/dist/css/bootstrap.min.css')
    link(rel='stylesheet',href='/components/font-awesome/css/font-awesome.min.css')
    link(rel='stylesheet',href='/stylesheets/animate.css')
    link(rel='stylesheet', href='/stylesheets/jackstrap.css')
    link(rel='stylesheet', href='/stylesheets/main.css')

    <meta property="og:image" content="http://i.imgur.com/e1dNQDT.png" />
    <meta property="og:title" content="Startup Shell" />
    <meta property="og:url" content="http://startupshell.org" />
    <meta property="og:site_name" content="Startup Shell" />
    <meta property="og:type" content="website" />
    <meta property="og:description" content="Startup Shell is a student-run nonprofit incubator that fosters entrepreneurship through collaboration at the University of Maryland." />

    script(src='/components/jquery/dist/jquery.min.js')
    script(src='/components/bootstrap/dist/js/bootstrap.min.js')
    script.
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
      ga('create', 'UA-43752415-1', 'startupshell.org');
      ga('send', 'pageview');

    

  body
    nav.navbar.navbar-default.navbar-fixed-top.header(role="navigation")
      .container
        .navbar-header
          button.navbar-toggle(type="button", data-toggle="collapse",data-target="#navigation")
            span.sr-only Toggle
            span.icon-bar
          a.navbar-brand(href="/"): img(src='/images/logo.svg')
          
        .collapse.navbar-collapse#navigation
          ul.nav.navbar-nav
            li: a(href="#about") about
            li: a(href="#ventures") ventures
            li: a(href="#connect") connect
            li: a(href="#membership") apply
    
    block content
    
    .footer.section
      .container
        .row
          .subscribe
              .container
                .row
                  .col-sm-12
                    input(id="subscribe" type="text" placeholder="subscribe to newsletter" autofocus)
          .col-sm-12
            .row
              .col-sm-6
                p &copy; 2013 Startup Shell, University of Maryland
              .col-sm-6.text-right
                ul
                  li
                    a(href="mailto:info@startupshell.org")
                      img(src="/images/icons/icon-mail.png", alt: "Email")
                  li
                    a(target="_blank" href="http://www.facebook.com/StartupShell")
                      img(src="/images/icons/icon-facebook.png", alt: "Facebook")
                  li
                    a(target="_blank" href="http://twitter.com/startup_shell")
                      img(src="/images/icons/icon-twitter.png", alt: "Twitter")

  
  script(src="javascripts/scroll.js")
  script(src="javascripts/getEvents.js")
